
generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
  relationMode = "prisma"
}
model Subscriber {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}


model Episode {
  id        Int      @id @default(autoincrement())
  picture   String
  link      String
  createdAt DateTime @default(now())
}
